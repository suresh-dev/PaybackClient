        <xsd:annotation>
          <xsd:documentation />
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="types:GetMemberCardDetailsByMobileOrEmailFaultResponse">
